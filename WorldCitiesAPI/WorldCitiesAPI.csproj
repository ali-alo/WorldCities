    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>c54608f5-e184-4f32-bdb6-db05d20d78e9</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPPlus" Version="4.5.3.3" />
